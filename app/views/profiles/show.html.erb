<% content_for :structured_data do %>
  <script type="application/ld+json">
    <%= profile_schema(@profile) %>
  </script>
  <script type="application/ld+json">
    <%= breadcrumb_schema([
      { name: 'Home', url: root_url },
      { name: 'Influencers', url: profiles_url },
      { name: @profile.full_name, url: profile_url(@profile) }
    ]) %>
  </script>
<% end %>

<!-- Hero Section - Slide 1 -->
<div class="min-h-screen bg-gradient-to-br from-slate-50 via-white to-blue-50">
  <main class="mx-auto container-fluid px-6 sm:px-8 lg:px-12 xl:px-16 2xl:px-24 py-12 lg:py-16">
    <div class="mx-auto max-w-[1600px]">
      <!-- Profile Hero -->
      <div class="lg:grid lg:grid-cols-2 lg:items-center lg:gap-x-16 mb-20">
        <!-- Image gallery -->
        <div class="relative group">
          <div class="absolute -inset-1 bg-gradient-to-r from-blue-600 to-purple-600 rounded-2xl blur opacity-25 group-hover:opacity-40 transition duration-300"></div>
          <div class="relative aspect-square w-full overflow-hidden rounded-2xl bg-white shadow-2xl ring-1 ring-gray-900/5">
            <div id="tabs-2-panel-1" aria-labelledby="tabs-2-tab-1" role="tabpanel" tabindex="0">
              <%=image_tag direct_blob_url(@profile.avatar), 
                                  class: 'h-full w-full object-cover object-center transform group-hover:scale-105 transition duration-500',
                                  alt: @profile.username%>
            </div>
          </div>
        </div>
        
        <!-- Profile info -->
        <div class="mt-10 lg:mt-0 space-y-8">
          <!-- Header -->
          <div class="space-y-4">
            <div class="flex items-center gap-3">
              <a href="https://www.instagram.com/<%=@profile.username%>" target="_blank" class="group inline-flex items-center gap-2 hover:opacity-80 transition-opacity">
                <h1 class="text-4xl lg:text-5xl font-bold tracking-tight text-gray-900">
                  <%=@profile.full_name%>
                </h1>
                <% if @profile.is_verified %>
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="#0095f6" class="w-7 h-7 flex-shrink-0">
                    <path fill-rule="evenodd" d="M16.403 12.652a3 3 0 0 0 0-5.304 3 3 0 0 0-3.75-3.751 3 3 0 0 0-5.305 0 3 3 0 0 0-3.751 3.75 3 3 0 0 0 0 5.305 3 3 0 0 0 3.75 3.751 3 3 0 0 0 5.305 0 3 3 0 0 0 3.751-3.75Zm-2.546-4.46a.75.75 0 0 0-1.214-.883l-3.483 4.79-1.88-1.88a.75.75 0 1 0-1.06 1.061l2.5 2.5a.75.75 0 0 0 1.137-.089l4-5.5Z" clip-rule="evenodd" />
                  </svg>
                <% end %>
              </a>
              <%if current_admin_user%>
                <%=link_to edit_admin_profile_path(@profile), target: '_blank', class: 'inline-flex items-center gap-2 rounded-lg bg-gray-100 px-4 py-2 text-sm font-medium text-gray-700 hover:bg-gray-200 transition-colors' do%>
                  <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4">
                    <path d="m5.433 13.917 1.262-3.155A4 4 0 0 1 7.58 9.42l6.92-6.918a2.121 2.121 0 0 1 3 3l-6.92 6.918c-.383.383-.84.685-1.343.886l-3.154 1.262a.5.5 0 0 1-.65-.65Z" />
                    <path d="M3.5 5.75c0-.69.56-1.25 1.25-1.25H10A.75.75 0 0 0 10 3H4.75A2.75 2.75 0 0 0 2 5.75v9.5A2.75 2.75 0 0 0 4.75 18h9.5A2.75 2.75 0 0 0 17 15.25V10a.75.75 0 0 0-1.5 0v5.25c0 .69-.56 1.25-1.25 1.25h-9.5c-.69 0-1.25-.56-1.25-1.25v-9.5Z" />
                  </svg>
                  Editar
                <%end%>
              <%end%>
            </div>
            
            <div class="flex items-center gap-2 text-lg text-gray-600">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5">
                <path d="M12.378 1.602a.75.75 0 00-.756 0L3 6.632l9 5.25 9-5.25-8.622-5.03zM21.75 7.93l-9 5.25v9l8.628-5.032a.75.75 0 00.372-.648V7.93zM11.25 22.18v-9l-9-5.25v8.57a.75.75 0 00.372.648l8.628 5.033z" />
              </svg>
              <a href="https://www.instagram.com/<%=@profile.username%>" target="_blank" class="font-medium hover:text-blue-600 transition-colors">
                @<%=@profile.username%>
              </a>
            </div>
          </div>

          <!-- Followers Stat Card -->
          <div class="relative overflow-hidden rounded-2xl bg-gradient-to-br from-blue-600 to-purple-600 p-8 shadow-xl">
            <div class="relative z-10">
              <p class="text-sm font-medium text-blue-100 uppercase tracking-wider">Audiencia Total</p>
              <p class="mt-2 text-5xl font-bold text-white">
                <%= number_to_human(@profile.followers, format: '%n%u', units: { thousand: 'K', million: 'M', billion: 'B' }, precision: 2) %>
              </p>
              <p class="mt-1 text-lg text-blue-100">Seguidores</p>
            </div>
            <div class="absolute top-0 right-0 -mt-4 -mr-4 h-32 w-32 rounded-full bg-white/10 blur-2xl"></div>
            <div class="absolute bottom-0 left-0 -mb-8 -ml-8 h-40 w-40 rounded-full bg-purple-500/20 blur-3xl"></div>
          </div>
          <!-- Biography Card -->
          <div class="rounded-xl bg-white border border-gray-200 p-6 shadow-sm">
            <h3 class="text-sm font-semibold text-gray-500 uppercase tracking-wider mb-3">Biograf√≠a</h3>
            <p class="text-base leading-relaxed text-gray-700"><%=@profile.biography%></p>
          </div>

          <!-- Performance Metrics -->
          <%if @profile.total_posts.positive?%>
            <div class="space-y-4">
              <!-- Engagement Card -->
              <div class="rounded-xl bg-gradient-to-br from-green-50 to-emerald-50 border border-green-200 p-6">
                <div class="flex items-start justify-between">
                  <div class="flex-1">
                    <h4 class="text-sm font-semibold text-green-800 uppercase tracking-wider mb-2">Engagement √öltima Semana</h4>
                    <%if @profile.total_posts == 1%>
                      <p class="text-sm leading-relaxed text-gray-700">
                        Cre√≥ <span class="font-bold text-green-700">1</span> publicaci√≥n, generando 
                        <span class="font-bold text-green-700"><%=number_with_delimiter(@profile.total_interactions_count, delimiter: '.')%></span> interacciones.
                      </p>
                    <%else%>
                      <p class="text-sm leading-relaxed text-gray-700 mb-2">
                        Cre√≥ <span class="font-bold text-green-700"><%=number_with_delimiter(@profile.total_posts, delimiter: '.')%></span> publicaciones con 
                        <span class="font-bold text-green-700"><%=number_with_delimiter(@profile.total_interactions_count, delimiter: '.')%></span> interacciones totales.
                      </p>
                      <p class="text-sm text-gray-600">
                        Promedio: <span class="font-semibold text-green-700"><%=number_with_delimiter(@median_interactions, delimiter: '.')%></span> interacciones/post
                      </p>
                    <%end%>
                  </div>
                  <div class="ml-4 flex-shrink-0">
                    <div class="inline-flex items-center rounded-full bg-green-600 px-4 py-2">
                      <span class="text-2xl font-bold text-white"><%=@profile.engagement_rate%>%</span>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Estimated Reach Card -->
              <div class="rounded-xl bg-gradient-to-br from-blue-50 to-indigo-50 border border-blue-200 p-6">
                <div class="flex items-start justify-between">
                  <div class="flex-1">
                    <h4 class="text-sm font-semibold text-blue-800 uppercase tracking-wider mb-2">Alcance Estimado</h4>
                    <p class="text-sm leading-relaxed text-gray-700 mb-2">
                      Sus publicaciones llegan aproximadamente a 
                      <span class="font-bold text-blue-700"><%=number_with_delimiter(@profile.estimated_reach, delimiter: '.')%></span> cuentas √∫nicas.
                    </p>
                    <p class="text-sm text-gray-600">
                      Esto representa el <span class="font-semibold text-blue-700"><%=@profile.estimated_reach_percentage%>%</span> de sus seguidores
                    </p>
                  </div>
                  <div class="ml-4 flex-shrink-0">
                    <div class="inline-flex items-center justify-center rounded-full bg-blue-600 w-16 h-16">
                      <span class="text-3xl">üéØ</span>
                    </div>
                  </div>
                </div>
                <!-- Progress Bar -->
                <div class="mt-4">
                  <div class="relative pt-1">
                    <div class="overflow-hidden h-2 text-xs flex rounded-full bg-blue-200">
                      <div style="width: <%=[@profile.estimated_reach_percentage, 100].min%>%" 
                           class="shadow-none flex flex-col text-center whitespace-nowrap text-white justify-center bg-gradient-to-r from-blue-500 to-indigo-600 transition-all duration-500">
                      </div>
                    </div>
                  </div>
                  <% avg_reach = 15.0 %>
                  <div class="mt-2 text-xs">
                    <% if @profile.estimated_reach_percentage > avg_reach + 5 %>
                      <span class="inline-flex items-center gap-1 text-green-700 font-semibold">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4">
                          <path fill-rule="evenodd" d="M10 17a.75.75 0 0 1-.75-.75V5.612L5.29 9.77a.75.75 0 0 1-1.08-1.04l5.25-5.5a.75.75 0 0 1 1.08 0l5.25 5.5a.75.75 0 1 1-1.08 1.04l-3.96-4.158V16.25A.75.75 0 0 1 10 17Z" clip-rule="evenodd" />
                        </svg>
                        +<%=(@profile.estimated_reach_percentage - avg_reach).round(1)%>% sobre el promedio
                      </span>
                    <% elsif @profile.estimated_reach_percentage < avg_reach - 3 %>
                      <span class="inline-flex items-center gap-1 text-orange-600 font-semibold">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4">
                          <path fill-rule="evenodd" d="M10 3a.75.75 0 0 1 .75.75v10.638l3.96-4.158a.75.75 0 1 1 1.08 1.04l-5.25 5.5a.75.75 0 0 1-1.08 0l-5.25-5.5a.75.75 0 1 1 1.08-1.04l3.96 4.158V3.75A.75.75 0 0 1 10 3Z" clip-rule="evenodd" />
                        </svg>
                        <%=(avg_reach - @profile.estimated_reach_percentage).round(1)%>% bajo el promedio
                      </span>
                    <% else %>
                      <span class="inline-flex items-center gap-1 text-gray-600 font-semibold">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4">
                          <path fill-rule="evenodd" d="M10 18a8 8 0 1 0 0-16 8 8 0 0 0 0 16Zm.75-13a.75.75 0 0 0-1.5 0v5c0 .414.336.75.75.75h4a.75.75 0 0 0 0-1.5h-3.25V5Z" clip-rule="evenodd" />
                        </svg>
                        En el promedio de la industria
                      </span>
                    <% end %>
                  </div>
                </div>
              </div>

              <!-- Video Performance Card -->
              <%if @profile.total_videos > 0%>
                <div class="rounded-xl bg-gradient-to-br from-purple-50 to-pink-50 border border-purple-200 p-6">
                  <h4 class="text-sm font-semibold text-purple-800 uppercase tracking-wider mb-2">Rendimiento Videos</h4>
                  <%if @profile.total_videos == 1%>
                    <p class="text-sm leading-relaxed text-gray-700">
                      Su video fue visto <span class="font-bold text-purple-700"><%=number_with_delimiter(@profile.total_video_view_count, delimiter: '.')%></span> veces.
                    </p>
                  <%else%>
                    <p class="text-sm leading-relaxed text-gray-700 mb-2">
                      <span class="font-bold text-purple-700"><%=number_with_delimiter(@profile.total_videos, delimiter: '.')%></span> videos con 
                      <span class="font-bold text-purple-700"><%=number_with_delimiter(@profile.total_video_view_count, delimiter: '.')%></span> visualizaciones.
                    </p>
                    <p class="text-sm text-gray-600">
                      Promedio: <span class="font-semibold text-purple-700"><%=number_with_delimiter(@median_video_views, delimiter: '.')%></span> views/video
                    </p>
                  <%end%>
                </div>
              <%else%>
                <div class="rounded-xl bg-gray-50 border border-gray-200 p-4">
                  <p class="text-sm text-gray-500 text-center">Sin videos publicados esta semana</p>
                </div>
              <%end%>
            </div>
          <%end%>
      </div>
    </div>
  </main>
</div>

<!-- Stats Section - Slide 2 -->
<%if @last_week_posts.any?%>
  <div class="min-h-screen bg-gradient-to-br from-white via-slate-50 to-gray-100 py-20">
    <div class="mx-auto px-6 sm:px-8 lg:px-12 xl:px-16 2xl:px-24 max-w-[1800px]">
      <!-- Section Header -->
      <div class="mb-12 text-center">
        <h2 class="text-4xl font-bold tracking-tight text-gray-900 sm:text-5xl">M√©tricas de Rendimiento</h2>
        <p class="mt-4 text-lg leading-relaxed text-gray-600 max-w-3xl mx-auto">
          An√°lisis completo de los √∫ltimos d√≠as
        </p>
        <div class="mt-4 flex items-center justify-center gap-2 text-sm text-gray-500">
          <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4">
            <path fill-rule="evenodd" d="M18 10a8 8 0 1 1-16 0 8 8 0 0 1 16 0Zm-7-4a1 1 0 1 1-2 0 1 1 0 0 1 2 0ZM9 9a.75.75 0 0 0 0 1.5h.253a.25.25 0 0 1 .244.304l-.459 2.066A1.75 1.75 0 0 0 10.747 15H11a.75.75 0 0 0 0-1.5h-.253a.25.25 0 0 1-.244-.304l.459-2.066A1.75 1.75 0 0 0 9.253 9H9Z" clip-rule="evenodd" />
          </svg>
          Likes y comentarios = interacciones | Visualizaciones de videos = m√©trica separada
        </div>
      </div>

      <!-- Stats Grid -->
      <dl class="grid grid-cols-1 gap-6 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-5 mb-20">
        <!-- Total Posts -->
        <div class="group relative overflow-hidden rounded-2xl bg-white p-8 shadow-lg ring-1 ring-gray-900/5 transition-all hover:shadow-xl hover:scale-105">
          <div class="absolute top-0 right-0 -mt-4 -mr-4 h-24 w-24 rounded-full bg-blue-100 opacity-20 group-hover:opacity-30 transition-opacity"></div>
          <dt class="relative text-sm font-semibold text-gray-500 uppercase tracking-wider">Total Posts</dt>
          <dd class="relative mt-3 flex items-baseline gap-2">
            <span class="text-5xl font-bold tracking-tight text-gray-900"><%=@last_week_posts.size%></span>
          </dd>
          <div class="mt-4 flex items-center text-sm text-gray-500">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4 mr-1">
              <path fill-rule="evenodd" d="M1 5.25A2.25 2.25 0 0 1 3.25 3h13.5A2.25 2.25 0 0 1 19 5.25v9.5A2.25 2.25 0 0 1 16.75 17H3.25A2.25 2.25 0 0 1 1 14.75v-9.5Zm1.5 5.81v3.69c0 .414.336.75.75.75h13.5a.75.75 0 0 0 .75-.75v-2.69l-2.22-2.219a.75.75 0 0 0-1.06 0l-1.91 1.909.47.47a.75.75 0 1 1-1.06 1.06L6.53 8.091a.75.75 0 0 0-1.06 0l-2.97 2.97ZM12 7a1 1 0 1 1-2 0 1 1 0 0 1 2 0Z" clip-rule="evenodd" />
            </svg>
            Publicaciones
          </div>
        </div>

        <!-- Total Interactions -->
        <div class="group relative overflow-hidden rounded-2xl bg-gradient-to-br from-green-500 to-emerald-600 p-8 shadow-lg transition-all hover:shadow-xl hover:scale-105">
          <div class="absolute bottom-0 left-0 -mb-8 -ml-8 h-32 w-32 rounded-full bg-white/10 blur-2xl"></div>
          <dt class="relative text-sm font-semibold text-green-100 uppercase tracking-wider">Total Interacciones</dt>
          <dd class="relative mt-3 flex items-baseline gap-2">
            <span class="text-5xl font-bold tracking-tight text-white">
              <%= number_to_human(@profile.total_interactions_count, format: '%n%u', units: { thousand: 'K', million: 'M', billion: 'B' }, precision: 2) %>
            </span>
          </dd>
          <div class="relative mt-4 flex items-center text-sm text-green-100">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4 mr-1">
              <path d="M1 8.25a1.25 1.25 0 1 1 2.5 0v7.5a1.25 1.25 0 1 1-2.5 0v-7.5ZM11 3V1.7c0-.268.14-.526.395-.607A2 2 0 0 1 14 3c0 .995-.182 1.948-.514 2.826-.204.54.166 1.174.744 1.174h2.52c1.243 0 2.261 1.01 2.146 2.247a23.864 23.864 0 0 1-1.341 5.974C17.153 16.323 16.072 17 14.9 17h-3.192a3 3 0 0 1-1.341-.317l-2.734-1.366A3 3 0 0 0 6.292 15H5V8h.963c.685 0 1.258-.483 1.612-1.068a4.011 4.011 0 0 1 2.166-1.73c.432-.143.853-.386 1.011-.814.16-.432.248-.9.248-1.388Z" />
            </svg>
            Likes + Comentarios
          </div>
        </div>

        <!-- Video Views -->
        <div class="group relative overflow-hidden rounded-2xl bg-gradient-to-br from-purple-500 to-pink-600 p-8 shadow-lg transition-all hover:shadow-xl hover:scale-105">
          <div class="absolute top-0 right-0 -mt-4 -mr-4 h-32 w-32 rounded-full bg-white/10 blur-2xl"></div>
          <dt class="relative text-sm font-semibold text-purple-100 uppercase tracking-wider">Visualizaciones Videos</dt>
          <dd class="relative mt-3 flex items-baseline gap-2">
            <span class="text-5xl font-bold tracking-tight text-white">
              <%= number_to_human(@profile.total_video_view_count, format: '%n%u', units: { thousand: 'K', million: 'M', billion: 'B' }, precision: 2) %>
            </span>
          </dd>
          <div class="relative mt-4 flex items-center text-sm text-purple-100">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4 mr-1">
              <path d="M3.25 4A2.25 2.25 0 0 0 1 6.25v7.5A2.25 2.25 0 0 0 3.25 16h7.5A2.25 2.25 0 0 0 13 13.75v-7.5A2.25 2.25 0 0 0 10.75 4h-7.5ZM19 4.75a.75.75 0 0 0-1.28-.53l-3 3a.75.75 0 0 0-.22.53v4.5c0 .199.079.39.22.53l3 3a.75.75 0 0 0 1.28-.53V4.75Z" />
            </svg>
            Reproducciones
          </div>
        </div>

        <!-- Followers -->
        <div class="group relative overflow-hidden rounded-2xl bg-white p-8 shadow-lg ring-1 ring-gray-900/5 transition-all hover:shadow-xl hover:scale-105">
          <div class="absolute bottom-0 right-0 -mb-6 -mr-6 h-28 w-28 rounded-full bg-indigo-100 opacity-20 group-hover:opacity-30 transition-opacity"></div>
          <dt class="relative text-sm font-semibold text-gray-500 uppercase tracking-wider">Seguidores</dt>
          <dd class="relative mt-3 flex items-baseline gap-2">
            <span class="text-5xl font-bold tracking-tight text-gray-900">
              <%= number_to_human(@profile.followers, format: '%n%u', units: { thousand: 'K', million: 'M', billion: 'B' }, precision: 2) %>
            </span>
          </dd>
          <div class="relative mt-4 flex items-center text-sm text-gray-500">
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4 mr-1">
              <path d="M10 9a3 3 0 1 0 0-6 3 3 0 0 0 0 6ZM6 8a2 2 0 1 1-4 0 2 2 0 0 1 4 0ZM1.49 15.326a.78.78 0 0 1-.358-.442 3 3 0 0 1 4.308-3.516 6.484 6.484 0 0 0-1.905 3.959c-.023.222-.014.442.025.654a4.97 4.97 0 0 1-2.07-.655ZM16.44 15.98a4.97 4.97 0 0 0 2.07-.654.78.78 0 0 0 .357-.442 3 3 0 0 0-4.308-3.517 6.484 6.484 0 0 1 1.907 3.96 2.32 2.32 0 0 1-.026.654ZM18 8a2 2 0 1 1-4 0 2 2 0 0 1 4 0ZM5.304 16.19a.844.844 0 0 1-.277-.71 5 5 0 0 1 9.947 0 .843.843 0 0 1-.277.71A6.975 6.975 0 0 1 10 18a6.974 6.974 0 0 1-4.696-1.81Z" />
            </svg>
            Audiencia
          </div>
        </div>

        <!-- Estimated Reach -->
        <div class="group relative overflow-hidden rounded-2xl bg-gradient-to-br from-blue-500 to-indigo-600 p-8 shadow-lg transition-all hover:shadow-xl hover:scale-105">
          <div class="absolute top-0 right-0 -mt-6 -mr-6 h-32 w-32 rounded-full bg-white/10 blur-2xl"></div>
          <dt class="relative text-sm font-semibold text-blue-100 uppercase tracking-wider">Alcance Estimado</dt>
          <dd class="relative mt-3 flex items-baseline gap-2">
            <span class="text-5xl font-bold tracking-tight text-white">
              <%= number_to_human(@profile.estimated_reach, format: '%n%u', units: { thousand: 'K', million: 'M', billion: 'B' }, precision: 2) %>
            </span>
          </dd>
          <div class="relative mt-4 flex items-center justify-between">
            <div class="flex items-center text-sm text-blue-100">
              <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4 mr-1">
                <path fill-rule="evenodd" d="M10 2c-1.716 0-3.408.106-5.07.31C3.806 2.45 3 3.414 3 4.517V17.25a.75.75 0 0 0 1.075.676L10 15.082l5.925 2.844A.75.75 0 0 0 17 17.25V4.517c0-1.103-.806-2.068-1.93-2.207A41.403 41.403 0 0 0 10 2Z" clip-rule="evenodd" />
              </svg>
              Cuentas √∫nicas
            </div>
            <span class="text-sm font-semibold text-white/90"><%=@profile.estimated_reach_percentage%>%</span>
          </div>
        </div>
      </dl>

      <%if current_admin_user && @collabs.any? && false%>
        <div class="mb-16">
          <h2 class="text-2xl font-bold text-gray-900 mb-6">√öltimas Colaboraciones</h2>
          <%= render partial: 'posts/collaborations', locals: { collaborations: @collabs } %>
        </div>
      <%end%>

      <!-- Word Cloud -->
      <%if @profile.instagram_posts.a_month_ago && @profile.instagram_posts.a_month_ago.word_occurrences.any?%>
        <div class="mb-16">
          <div class="rounded-2xl bg-white p-10 shadow-xl ring-1 ring-gray-900/5">
            <div class="flex items-center justify-between mb-8">
              <div>
                <h2 class="text-3xl font-bold text-gray-900">Nube de Palabras</h2>
